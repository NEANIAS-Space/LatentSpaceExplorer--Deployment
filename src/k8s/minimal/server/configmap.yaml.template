apiVersion: v1
kind: ConfigMap
metadata:
  name: server-config
  labels:
    k8s-app: server
data:
  .env: |-

    NEXCLOUD_HOST=#e.g. https://b2drop.eudat.eu
    NEXCLOUD_USER=#The one you get from nextcloud create app password step
    NEXCLOUD_PASSWORD=#The one you get from nextcloud create app password step

    APP_SERVER_HOST=0.0.0.0
    APP_SERVER_PORT=8080
    APP_SERVER_ROOT_PATH=/server
    APP_SERVER_RELOAD=True

    
